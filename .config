# Print the generated docker command
CRUISE_DEBUG_ENABLED="yes"
# Stop after printing the command
STOP_ON_DEBUG="no"

# Cruise defaults
DEFAULT_IMAGE_VERSION=8.1
DEFAULT_NETWORK=laravel-cruise
DEFAULT_PORT=80

# Docker defaults
CONTAINER_NAME_PATTERN=${PWD##*/}-cruise-$DEFAULT_IMAGE_VERSION-dev
WORKING_DIRECTORY=/var/www/html
ENTRYPOINT=zsh
