# Print the generated docker command
CRUISE_DEBUG_ENABLED="yes"
# Stop after printing the command
STOP_ON_DEBUG="no"

# Cruise defaults
DEFAULT_IMAGE_VERSION=8.4
DEFAULT_SHELL_USER=cruise
DEFAULT_NETWORK=cruise
DEFAULT_PORT=80

# Traefik defaults
TRAEFIK_VERSION=3.5
TRAEFIK_CONTAINER_NAME=traefik

# Docker defaults
CONTAINER_NAME_PATTERN=${PWD##*/}-cruise-$DEFAULT_IMAGE_VERSION-dev
WORKING_DIRECTORY=/var/www/html
ENTRYPOINT=zsh
